{
    "version": 2,
    "builds": [
        {
            "src": "WebApp/wsgi.py",
            "use": "@vercel/python",
            "config": {"maxLambdaSize":  "15mb", "runtime":  "python3.9"}
        },
        {
            "src": "build_files.sh",
            "use": "@vercel/static_build",
            "config": {
                "distDir":  "staticfiles_build"
            }
        }
    ],
    "routes": [
      {
          "src": "/(.*)",
          "dest": "WebApp/wsgi.py"
      },
      {
          "src": "/(.*)",
          "dest": "WebApp/wsgi.py"
      }
    ]
}